  "channelSettingsSchema": {
    "configDef": {
      "configModel": {
        "type": "object",
        "title": "iQmetrix Dropship Channel Settings",
        "properties": {
          "protocol": {
            "title": "Protocol",
            "type": "string"
          },
          "implFolderName": {
            "title": "Implementation Folder Name",
            "type": "string"
          },
          "environment": {
            "title": "Environment",
            "type": "string"
          },
          "maxParallelRequests": {
            "title": "Max number of parallel requests",
            "type": "integer"
          },
          "canPostInvoice": {
            "title": "canPostInvoice",
            "description": "POST a Sales Order and Create an Invoice in RQ (SaleInvoice).  OR Submit a Sales Order with No Invoice (SaleOrder).",
            "type": "string",
            "enum": [
              "SaleInvoice",
              "SaleOrder"
            ]
          },
          "subscriptionLists": {
            "title": "Subscription Lists",
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "object",
              "properties": {
                "listId": {
                  "title": "List ID",
                  "type": "string"
                },
                "supplierId": {
                  "title": "Supplier ID",
                  "type": "integer"
                }
              },
              "required": [
                "listId",
                "supplierId"
              ]
            }
          }
        },
        "required": [
          "protocol",
          "implFolderName",
          "environment",
          "maxParallelRequests",
          "canPostInvoice",
          "subscriptionLists"
        ]
      },
      "configView": [
        "protocol",
        "environment",
        "maxParallelRequests",
        "canPostInvoice",
        "subscriptionLists"
      ],
      "configValues": {
        "protocol": "https",
        "implFolderName": "iqmetrix-dropship-channel",
        "environment": "demo"
      }
    }
  },
