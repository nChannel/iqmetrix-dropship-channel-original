            "type": "string"
          },
          "grant_type": {
            "type": "string"
          },
          "access_token": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password",
          "company_id",
          "location_id",
          "client_id",
          "client_secret",
          "grant_type",
          "access_token"
        ]
      },
      "configView": [
        {
          "key": "company_id",
          "title": "Company ID *"
        },
        {
          "key": "location_id",
          "title": "Location ID *"
        },
        {
          "key": "username",
          "title": "Username *"
        },
        {
          "key": "password",
          "type": "password",
          "title": "Password *"
        },
        {
          "key": "client_id",
          "title": "Client ID *"
        },
        {
          "key": "client_secret",
          "title": "Client Secret *"
        }
      ],
      "configValues": {
        "grant_type": "password"
      }
    },
    "oauthAuthorizeUrl": "",
    "oauthTokenRequest": {
      "verb": "POST",
      "url": "https://accountsdemo.iqmetrix.net/v1/oauth2/token",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "body": {
        "username": "{{username}}",
        "password": "{{password}}",
        "client_id": "{{client_id}}",
        "client_secret": "{{client_secret}}",
        "grant_type": "{{grant_type}}"
      }
    },
    "oauthTokenResponse": {
      "access_token": "access_token",
      "expires_in": "expires_in",
      "refresh_token": "refresh_token",
      "expirationConfig": {
        "func": "add",
        "args": [
          "expires_in",
          "s"
        ]
      }
    },
    "oauthRefreshTokenRequest": {
      "verb": "POST",
      "url": "https://accountsdemo.iqmetrix.net/v1/oauth2/token",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "body": {
        "refresh_token": "{{username}}",
        "client_id": "{{client_id}}",
        "client_secret": "{{client_secret}}",
        "grant_type": "refresh_token"
      }
    },
    "oauthRefreshTokenResponse": {
      "access_token": "access_token",
      "expires_in": "expires_in",
      "refresh_token": "refresh_token",
      "expirationConfig": {
        "func": "add",
        "args": [
          "expires_in",
          "s"
        ]
      }
    },
    "apps": [],
    "oauthUserRequest": {
      "verb": "GET",
      "url": "",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    "oauthUserResponse": {
      "user": {},
      "account": {}
    },
    "oauthIdentifier": "{{client_id}}"
